{
  "network": "livenet",
  "port": 3001,
  "services": [
    "ravend",
    "web",
    "insight-api",
    "insight-ui"
  ],
  "allowedOriginRegexp": "^https://(.+\\.)*raven\\.network$",
  "messageLog": "",
  "servicesConfig": {
    "web": {
      "disablePolling": true
    },
    "insight-ui": {
      "routePrefix": "",
      "apiPrefix": "api"
    },
    "insight-api": {
      "routePrefix": "api",
      "coinTicker" : "https://api.coinmarketcap.com/v1/ticker/ravencoin/?convert=USD",
      "coinShort": "RVN"
    },
    "ravend": {
      "sendTxLog": "/opt/underdarkskies/ravencore-rvn/home/.ravencore/pushtx.log",
      "spawn": {
        "datadir": "/opt/underdarkskies/ravencore-rvn/home/.ravencore/data",
        "exec": "/opt/underdarkskies/ravencore-rvn/ravencore/node_modules/ravencore-node/bin/ravend",
        "rpcqueue": 1000,
        "rpcport": 8766,
        "zmqpubrawtx": "tcp://127.0.0.1:28332",
        "zmqpubhashblock": "tcp://127.0.0.1:28332"
      }
    }
  }
}
