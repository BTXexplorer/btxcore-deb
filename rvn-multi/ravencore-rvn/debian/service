[Unit]
Description=underdarkskies ravencore-rvn

[Service]
ExecStart=/usr/bin/env nodejs --max_old_space_size=4096 /opt/underdarkskies/ravencore-rvn/ravencore/bin/ravencored
Restart=always
RestartSec=5
TimeoutStopSec=900
User=ravencore-rvn
WorkingDirectory=/opt/underdarkskies/ravencore-rvn/home

[Install]
WantedBy=multi-user.target
